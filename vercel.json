{
    "version": 2,
    "projects": [
      {
        "src": "./frontend",
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      },
      {
        "src": "./backend",
        "use": "@vercel/python",
        "buildCommand": "pip install -r requirements.txt",
        "outputDirectory": "."
      }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/backend/$1" }
    ]
  }
  